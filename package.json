{
  "name": "lean-react-starter",
  "version": "0.4.3",
  "description": "",
  "main": "src/script.jsx",
  "scripts": {
    "build": "webpack --env production",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "format": "prettier --write --parser=flow --print-width=120 --single-quote --tab-width=2 \"src/**/*.{js,jsx}\"",
    "lint:js": "eslint --cache src/**/*.{js,jsx}",
    "lint:js:fix": "eslint --cache --fix src/**/*.{js,jsx}",
    "lint:style": "stylelint src/**/*.css src/**/*.scss",
    "preview": "http-server ./build -p 9001",
    "start": "nodemon --watch webpack.config.babel.js --watch webpack.parts.js --exec \"webpack-dev-server --env dev\"",
    "start:debug": "node --inspect-brk node_modules/.bin/webpack --env debug",
    "stats": "webpack --env production --profile --json > stats.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
